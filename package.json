{
  "name": "electron-elm-boilerplate",
  "version": "0.0.0",
  "description": "description",
  "main": "index.js",
  "scripts": {
    "dist": "build",
    "open": "electron .",
    "serve": "elm-live --debug --warn src/Main.elm --output elm.js",
    "start": "npm run serve | npm run open",
    "build": "elm-make --warn src/Main.elm --output elm.js",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tshm/electron-elm-boilerplate.git"
  },
  "keywords": [
    "elm",
    "electron",
    "boilerplate"
  ],
  "author": "tosh satake",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tshm/electron-elm-boilerplate/issues"
  },
  "homepage": "https://github.com/tshm/electron-elm-boilerplate#readme",
  "dependencies": {
  },
  "devDependencies": {
    "electron": "^1.4.15",
    "electron-builder": "^14.5.3",
    "elm": "^0.18.0",
    "elm-live": "^2.7.4"
  },
  "build": {
    "appId": "electron-elm-boilerplate",
    "copyright": "Copyright 2016-2017 WHO",
    "files": [
      "!.*",
      "!elm-stuff/**/*",
      "!tmp/**/*",
      "!src/**/*"
    ]
  }
}
